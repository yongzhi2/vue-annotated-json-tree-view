{
  "name": "vue-annotated-json-tree-view",
  "version": "2.1.6",
  "repository": "yongzhi2/vue-annotated-json-tree-view",
  "homepage": "https://github.com/yongzhi2/vue-annotated-json-tree-view",
  "bugs": {
    "url": "https://github.com/yongzhi2/vue-annotated-json-tree-view/issues",
    "email": "mail@arvidkahl.de"
  },
  "description": "Annotated JSON Tree View Component in Vue.js",
  "main": "./dist/vue-json-tree-view.min.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --watch --progress --config webpack.dev.config.js --port 5000",
    "build": "webpack --progress --config webpack.prod.config.js",
    "test": "jest --coverage",
    "ci": "npm run test && npm run build"
  },
  "keywords": [
    "vue-component",
    "vuejs"
  ],
  "author": "Arvid Kahl <mail@arvidkahl.de>",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.4",
    "vue": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-jest": "^22.0.4",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-runtime": "^6.9.2",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-config-google": "^0.6.0",
    "eslint-config-vue": "^1.0.3",
    "eslint-plugin-html": "^1.5.1",
    "file-loader": "^0.8.5",
    "isparta": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "istanbul": "^0.4.4",
    "jest": "^22.0.4",
    "jest-serializer-vue": "^0.3.0",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^1.3.2",
    "vue-html-loader": "^1.1.0",
    "vue-jest": "^1.4.0",
    "vue-loader": "^13.6.2",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.4.2",
    "vue-test-utils": "^1.0.0-beta.9",
    "webpack": "^2.1.0-beta.2",
    "webpack-dev-server": "^1.15.0",
    "webpack-merge": "^0.14.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "mapCoverage": true
  }
}
